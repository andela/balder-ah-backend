{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "npm-run-all --parallel clean build migrate",
    "dev:start": "npm run dev && npm run start",
    "dev:start:watch": "nodemon --watch src --exec 'npm run dev:start'",
    "start": "node dist/server.js",
    "build": "babel ./src -d dist",
    "migrate": "sequelize db:migrate",
    "migrate:test": "sequelize db:migrate:undo:all && npm run migrate",
    "postinstall": "npm run dev",
    "test": "export NODE_ENV=test && npm run migrate:test && nyc --reporter=html --reporter=text mocha --recursive --require babel-register --exit",
    "test:watch": "nodemon --watch test --exec npm test",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "babel-register": "6.26.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "morgan": "1.9.1",
    "pg": "7.6.1",
    "pg-hstore": "2.3.2",
    "request": "^2.87.0",
    "sequelize": "4.41.2",
    "slug": "^0.9.2",
    "swagger-ui-express": "4.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-add-module-exports": "1.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "4.2.0",
    "chai-http": "4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "5.2.0",
    "nodemon": "1.18.7",
    "npm-run-all": "4.1.5",
    "nyc": "^13.1.0",
    "rimraf": "2.6.2",
    "sequelize-cli": "5.3.0",
    "sinon": "7.1.1",
    "sinon-chai": "3.3.0"
  }
}
