    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "morgan": "1.9.1",
    "pg": "7.6.1",
    "pg-hstore": "2.3.2",
    "request": "^2.87.0",
    "sequelize": "4.41.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "1.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "4.2.0",
    "chai-http": "4.2.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.5",
    "rimraf": "2.6.2",
    "sequelize-cli": "5.3.0"
